{
  "expo": {
    "name": "Ghici",
    "slug": "guess-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/book.png",
    "splash": {
      "image": "./assets/splash_1.png",
      "resizeMode": "contain",
      "backgroundColor": "#008000"
    },
    "updates": {
      "fallbackToCacheTimeout": 3000
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "ro.andreipirea.guess_app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/book.png",
        "backgroundColor": "#008000"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
